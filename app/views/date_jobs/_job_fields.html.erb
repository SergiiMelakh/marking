<fieldset>
        <%= f.collection_select(:line_id, Line.order(:id), :id, :name) %>
        <%= f.text_field :square, size: 13 %>
        <%= f.collection_select(:street_id, Street.order(:id), :id, :street) %>
        <%= f.text_field :house, size: 10 %>
        <%= f.text_field :note, size: 45 %>
        <%= f.hidden_field :_destroy %><%= link_to "Удалить", '#', class: "remove_fields" %>
</fieldset>