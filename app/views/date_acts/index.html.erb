<p id="notice"><%= notice %></p>

<h1>Listing Date Acts</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @date_acts.each do |date_act| %>
      <tr>
        <td><%= date_act.date %></td>
        <td><%= link_to 'Show', date_act %></td>
        <td><%= link_to 'Edit', edit_date_act_path(date_act) %></td>
        <td><%= link_to 'Destroy', date_act, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Date act', new_date_act_path %>
